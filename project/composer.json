{
    "name": "perfumerlabs/box",
    "type": "library",
    "description": "Docker container for intermediate storage with API",
    "homepage": "https://perfumerlabs.com",
    "authors": [
        {
            "name": "Ilyas Makashev",
            "email": "mehmatovec@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "7.4.*",
        "perfumer/framework": "0.9.2",
        "propel/propel": "2.0.0-alpha11",
        "envms/fluentpdo": "2.*",
        "pwfisher/command-line-php": "dev-master",
        "symfony/console": "5.*",
        "symfony/http-foundation": "5.*",
        "nikic/fast-route": "1.*",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "filp/whoops": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Project\\": "src/",
            "Box\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "platform":{
            "php":"7.4"
        }
    }
}
