{
    "name": "perfumerlabs/box",
    "type": "library",
    "description": "Docker container for intermediate storage with API",
    "homepage": "https://perfumerlabs.com",
    "authors": [
        {
            "name": "Ilyas Makashev",
            "email": "mehmatovec@gmail.com",
            "role": "Maintainer"
        }
    ],
    "repositories": [
        {
            "type": "gitlab",
            "url":  "https://git.perfumerlabs.com/perfumer/microservices-ee"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "ext-pdo": "*",
        "perfumer/framework": "~0.10",
        "propel/propel": "2.0.0-alpha12",
        "pwfisher/command-line-php": "dev-master",
        "symfony/console": "5.*",
        "symfony/http-foundation": "5.*",
        "nikic/fast-route": "1.*",
        "guzzlehttp/guzzle": "6.*",
        "ramsey/uuid": "~4",
        "perfumer/microservices": "dev-dev as v1.2.0",
        "perfumer/microservices-ee": "dev-master"
    },
    "require-dev": {
        "filp/whoops": "2.*",
        "perfumerlabs/perfumer-framework-pack": "~0",
        "codeception/codeception": "*",
        "codeception/module-rest": "*",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Model\\": "src/Model/App/",
            "Project\\": "src/",
            "Box\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "gitlab-domains": ["git.perfumerlabs.com"],
        "gitlab-token": {
            "git.perfumerlabs.com": "8BBR6hJppjcxvFeUzHpH"
        }
    }
}
